<div class=".col-md-6 .col-md-offset-3">
        <div>
          <span class="weather">
	          CURRENT LOCATION: <span class="city"><strong>{{city.name}}, {{city.country}}</strong></span>
	        </span>
        </div>
        <hr>
        <div id="city_weather">
            <div class="coord">
                 <span>Lat: <strong>{{city.coord.lat}}</strong> and Lon: <strong>{{city.coord.lon}}</strong></span>
            </div>
            <br>
            <div class="week_temps">
                <span>5 DAYS FORECAST: </span>
            </div>
            <hr>
            <div class="row">
